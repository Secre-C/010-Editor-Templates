{
	s32 Unk0;
	f32 Unk1;
	f32 Unk2;
	s32 CueID;
	s32 Unk4;
	s32 Unk5;
	s32 Unk6;
	s32 Unk7;
	s32 Unk8;
	s16 Unk9;
	s16 Unk10;
