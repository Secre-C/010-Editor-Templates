	u32 Unk1;
	u32 Entry_Num<name="Number of Entries">;
	u32 Unk2;
	u32 Unk3;
	u32 Unk4;
}DataHeader<optimize=false>;

typedef struct
{
	char FileName[128];	
}FileStringSec<optimize=false>;

typedef struct
{
	s32 Unk0;
