}Type4Data<optimize=false>;


typedef struct
{
	u32 Type;
	u32 Version<name="Version">;
	u32 SectionSize<name="Section Size">;
	u32 Unk1;
	if (Type == 1)
	{
		FileStringSec AWB;
		FileStringSec ACB;
	}
	else
	{
		u32 Entry_Num<name="Number of Entries">;
		u32 Unk2;
		u32 Unk3;
		u32 Unk4;
	}
	if (Type == 2)
	{
		Type2Data Type_2_Data[Entry_Num];
	}
	else if (Type == 3)
	{
		Type3Data Type_3_Data[Entry_Num];
	}
	else if (Type == 4)
	{
		Type4Data Type_4_Data[Entry_Num];
	}
	
	
}SectionData<optimize=false>;

typedef struct
{ 
    u16 CharModel;
}Crowd<optimize=false, read=TypeRead>;

string TypeRead(Crowd& block )
{
    char buffer[32];
    SPrintf( buffer, "Character %02d", block.CharModel );
    return buffer;
}

